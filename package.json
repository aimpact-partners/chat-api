{
	"name": "@aimpact/chat-api",
	"version": "0.1.0",
	"title": "CHAT API for custom chats using Agents",
	"description": "",
	"license": "ISC",
	"scripts": {
		"start": "node index.js"
	},
	"languages": {
		"default": "en",
		"supported": [
			"en",
			"es"
		]
	},
	"modules": {
		"path": "modules"
	},
	"params": {
		"local": {
			"AGENT_API": "http://localhost:5050"
		},
		"development": {
			"AGENT_API": "https://agent-api-rb5caohzgq-uc.a.run.app/agent"
		},
		"testing": {
			"AGENT_API": "https://agent-api-rb5caohzgq-uc.a.run.app/agent"
		}
	},
	"libraries": {
		"imports": [
			"@beyond-js/reactive"
		]
	},
	"deployment": {
		"distributions": [
			{
				"name": "web",
				"environment": "development",
				"platform": "web",
				"backend": "backend",
				"ports": {
					"bundles": 3380,
					"http": 3581,
					"inspect": 3282
				}
			},
			{
				"name": "backend",
				"environment": "development",
				"platform": "backend",
				"host": "https://chat-api-ws-v2-rb5caohzgq-uc.a.run.app",
				"ports": {
					"bundles": 6580,
					"http": 6581,
					"inspect": 6582
				}
			},
			{
				"name": "http",
				"environment": "development",
				"platform": "node",
				"ports": {
					"bundles": 6583,
					"http": 6584,
					"inspect": 6585
				}
			},
			{
				"name": "web-testing",
				"environment": "testing",
				"platform": "web",
				"backend": "backend-testing",
				"ports": {
					"bundles": 3385,
					"http": 3586,
					"inspect": 3287
				}
			},
			{
				"name": "backend-testing",
				"environment": "testing",
				"platform": "backend",
				"host": "https://chat-api-ws-skvkre4apq-uc.a.run.app",
				"ports": {
					"bundles": 6590,
					"http": 6591,
					"inspect": 6592
				}
			},
			{
				"name": "http-testing",
				"environment": "testing",
				"platform": "node",
				"ports": {
					"bundles": 6593,
					"http": 6594,
					"inspect": 6595
				}
			},
			{
				"name": "node-tsc",
				"environment": "local",
				"platform": "node",
				"ts": {
					"compiler": "tsc"
				},
				"ports": {
					"bundles": 6501
				}
			}
		]
	},
	"dependencies": {
		"@beyond-js/backend": "^0.1.8",
		"@beyond-js/events": "^0.0.6",
		"@beyond-js/kernel": "~0.1.8",
		"@beyond-js/firestore-collection": "~0.0.2",
		"@beyond-js/response": "~0.0.1",
		"@google-cloud/storage": "^6.10.1",
		"@google-cloud/tasks": "^3.1.2",
		"@pinecone-database/pinecone": "^0.1.6",
		"langchain": "^0.0.145",
		"axios": "^1.4.0",
		"busboy": "^1.6.0",
		"dayjs": "^1.11.7",
		"dexie": "^3.2.4",
		"dotenv": "^16.0.3",
		"express": "^4.17.1",
		"express-openapi-validator": "^5.0.4",
		"firebase": "^8.6.8",
		"firebase-admin": "^11.4.1",
		"fluent-ffmpeg": "^2.1.2",
		"form-data": "^4.0.0",
		"google-auth-library": "^8.9.0",
		"jsonwebtoken": "^9.0.1",
		"node-fetch": "^2.6.12",
		"openai": "~4.5.0",
		"socket.io-client": "^4.5.4",
		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"@beyond-js/bee": "0.0.6",
		"@beyond-js/local": "~0.1.3",
		"@types/jsonwebtoken": "^9.0.2",
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.9",
		"swagger-ui-express": "^5.0.0",
		"yaml": "^2.3.1"
	}
}
