{
	"name": "@aimpact/agents-api",
	"version": "0.4.0",
	"title": "Agents API for custom chats using Agents",
	"description": "",
	"license": "ISC",
	"scripts": {
		"start": "node index.js"
	},
	"languages": {
		"default": "en",
		"supported": [
			"en",
			"es"
		]
	},
	"modules": {
		"path": "modules"
	},
	"deployment": {
		"distributions": [
			{
				"name": "http",
				"environment": "local",
				"platform": "node",
				"ports": {
					"bundles": 6583,
					"http": 6584,
					"inspect": 6585
				}
			},
			{
				"name": "http-development",
				"environment": "development",
				"platform": "node"
			},
			{
				"name": "http-testing",
				"environment": "testing",
				"platform": "node"
			},
			{
				"name": "http-beta",
				"environment": "quality",
				"platform": "node"
			},
			{
				"name": "http-production",
				"environment": "production",
				"platform": "node"
			},
			{
				"name": "node-tsc",
				"environment": "local",
				"platform": "node",
				"ts": {
					"compiler": "tsc"
				},
				"ports": {
					"bundles": 6551
				}
			}
		]
	},
	"dependencies": {
		"@beyond-js/backend": "^0.1.9",
		"@beyond-js/firestore-collection": "~0.0.9",
		"@beyond-js/response": "~0.0.3",
		"@beyond-js/api-server": "~0.0.4",
		"@google-cloud/storage": "^6.10.1",
		"@google-cloud/tasks": "^5.3.0",
		"@pinecone-database/pinecone": "^0.1.6",
		"busboy": "^1.6.0",
		"dayjs": "^1.11.7",
		"dotenv": "^16.0.3",
		"firebase-admin": "^12.1.0",
		"fluent-ffmpeg": "^2.1.2",
		"form-data": "^4.0.0",
		"jsonwebtoken": "^9.0.1",
		"node-fetch": "^2.6.12",
		"openai": "~4.72.0",
		"uuid": "^9.0.0",
		"socket.io-client": "^4.5.4"
	},
	"devDependencies": {
		"@beyond-js/bee": "0.0.6",
		"@beyond-js/local": "~0.1.3",
		"@types/busboy": "1.5.3",
		"@types/jsonwebtoken": "^9.0.2",
		"@types/node": "~20.6.0",
		"@types/uuid": "~9.0.7",
		"swagger-ui-express": "^5.0.0",
		"yaml": "^2.3.1"
	}
}
