{
  "name": "@aimpact/chat-api",
  "version": "0.0.1",
  "title": "CHAT API for custom chats using Agents",
  "description": "",
  "license": "ISC",
  "languages": {
    "default": "en",
    "supported": [
      "en",
      "es"
    ]
  },
  "modules": {
    "path": "modules"
  },
  "deployment": {
    "distributions": [
